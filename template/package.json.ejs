{
    "name": "<%= appName %>",
    "version": "1.0.0",
    "description": "",
    "homepage": "",
    "author": {
      "name": "<%= name %>",
      "email": "<%= email %>",
      "url": ""
    },
    "main": "index.js",
    "scripts": {
      "precommit": "npm run precheck",
      "precheck": "npm run eslint && npm run csslint",
      "eslint": "cross-env NODE_ENV=test eslint --format node_modules/eslint-friendly-formatter --cache --ext .jsx,.js src/",
      "csslint": "stylelint src/**/*.scss --syntax scss",
      <%_ if (locals.version){ -%>
      "version": "node ./dev/version.js",
      <%_ } -%>
      "start": "<%_if (locals.version){%>npm run version && <%}%>node ./dev/server.dev.js",
      <%_ if (locals.unit){ -%>
      "bage": "node ./dev/bage.js",
      "test": "cross-env NODE_ENV=test karma start ./dev/karma.conf.js",
      <%_ } -%>
      "dstart": "cross-env NODE_ENV=production node ./dev/server.dist.js",
      "dist": "rimraf dist/ &&<%_if (locals.version){%> cross-env NODE_ENV=production npm run version &&<%}%> cross-env NODE_ENV=production webpack --config ./dev/webpack.dist.js && npm run dstart"
    },
    "dependencies": {
      "history": "^4.7.2",
      "react": "^16.0.0",
      "react-dom": "^16.0.0",
      <%_ if (locals.redux){ -%>
      "redux": "^3.7.2",
      "react-redux": "^5.0.6",
      "redux-thunk": "^2.2.0",
      <%_ } -%>
      <%_ if (locals.router){ -%>
      "react-router-dom": "^4.2.2",
      <%_ } -%>
      <%_ if (locals.router && locals.redux){ -%>
      "react-router-redux": "next",
      <%_ } -%>
      "prop-types": "^15.6.0"
    },
    "devDependencies": {
      "react-hot-loader": "^3.1.1",
      "autoprefixer": "^7.1.5",
      "babel-core": "^6.26.0",
      "babel-eslint": "^8.0.1",
      "babel-loader": "^7.1.2",
      "babel-plugin-transform-decorators-legacy": "^1.3.4",
      "babel-preset-es2015": "^6.24.1",
      "babel-preset-react": "^6.24.1",
      "babel-preset-stage-1": "^6.24.1",
      <%_ if(locals.cssm) { -%>
      "babel-plugin-react-css-modules-modify": "^1.0.3",
      <%_ } -%>
      "chalk": "^2.1.0",
      "copy-webpack-plugin": "^4.1.1",
      "cross-env": "^5.0.5",
      "css-loader": "^0.28.7",
      "eslint": "^4.8.0",
      "eslint-config-airbnb": "^16.0.0",
      "eslint-plugin-import": "^2.7.0",
      "eslint-plugin-jsx-a11y": "^6.0.2",
      "eslint-plugin-react": "^7.4.0",
      "eslint-friendly-formatter": "^3.0.0",
      "extract-text-webpack-plugin": "^3.0.1",
      "file-loader": "^1.1.5",
      "html-asset-webpack-plugin": "^1.0.1",
      "html-webpack-plugin": "^2.30.1",
      "node-sass": "^4.5.3",
      "postcss-loader": "^2.0.6",
      "precss": "^2.0.0",
      "pushstate-server": "^3.0.1",
      "rimraf": "^2.6.2",
      "sass-loader": "^6.0.6",
      "style-loader": "^0.19.0",
      "stylelint": "^8.2.0",
      "stylelint-config-standard": "^17.0.0",
      "url-loader": "^0.6.2",
      "webpack": "^3.6.0",
      "webpack-merge": "^4.1.0",
      "webpack-dev-server": "2.7.1",
      "stylelint-declaration-use-variable": "^1.6.0",
      "stylelint-scss": "^2.1.0",
      <%_ if (locals.unit){ -%>
      "babel-plugin-istanbul": "^4.1.3",
      "chai": "^3.5.0",
      "mocha": "^3.3.0",
      "karma": "^1.6.0",
      "karma-chai": "^0.1.0",
      "karma-chrome-launcher": "^2.1.0",
      "karma-coverage": "^1.1.1",
      "karma-mocha": "^1.3.0",
      "karma-sourcemap-loader": "^0.3.7",
      "karma-webpack": "^2.0.3",
      <%_ } -%>
      "ip": "^1.1.5"
    },
    "repository": "",
    "license": "MIT"
}
